// Test de string interpolation con función f()

funcion main()
    variable nombre = "Juan"
    variable edad = 30
    
    // Test básico
    mostrar(f("Hola {}", nombre))
    
    // Múltiples valores
    mostrar(f("Hola {}, tienes {} años", nombre, edad))
    
    // Con expresiones
    variable x = 10
    variable num2 = 5
    mostrar(f("La suma de {} + {} es {}", x, num2, x + num2))
    
    // Más complejo
    mostrar(f("{} tiene {} años", nombre, edad))
fin
