// Ejemplo simple de listas en Jade

funcion main()
    // Crear una lista
    variable numeros = [1, 2, 3, 4, 5]
    
    // Mostrar elementos
    mostrar("Lista creada")
    mostrar("Primer elemento: " + convertir_a_texto(numeros[0]))
    mostrar("Segundo elemento: " + convertir_a_texto(numeros[1]))
    
    // Modificar elemento
    numeros[0] = 10
    mostrar("Modificado primer elemento: " + convertir_a_texto(numeros[0]))
    
    // Usar m√©todos de lista
    numeros.agregar(6)
    mostrar("Elemento agregado. Longitud: " + convertir_a_texto(numeros.longitud()))
    
    variable eliminado = numeros.eliminar(0)
    mostrar("Elemento eliminado: " + convertir_a_texto(eliminado))
    mostrar("Nueva longitud: " + convertir_a_texto(numeros.longitud()))
fin
